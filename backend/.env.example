# Environnement
NODE_ENV=development
PORT=5000

# Base de données PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=paiecashfan_db
DB_USER=paiecashfan_user
DB_PASSWORD=votre_mot_de_passe_securise

# JWT Secret Keys (GÉNÉRER DES CLÉS UNIQUES EN PRODUCTION)
JWT_SECRET=votre_secret_jwt_ultra_securise_a_changer_en_production
JWT_REFRESH_SECRET=votre_refresh_secret_jwt_ultra_securise_a_changer_en_production
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# Chiffrement AES-256 (GÉNÉRER UNE CLÉ UNIQUE EN PRODUCTION)
ENCRYPTION_KEY=votre_cle_de_chiffrement_32_caracteres_exactement_12345678901234
ENCRYPTION_IV=votre_iv_16_car

# WalletConnect
WALLETCONNECT_PROJECT_ID=votre_project_id_walletconnect

# Infura (pour interactions blockchain)
INFURA_API_KEY=votre_cle_api_infura

# Email (Nodemailer avec Gmail ou SMTP)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=votre.email@gmail.com
EMAIL_PASSWORD=votre_mot_de_passe_application
EMAIL_FROM=noreply@paiecashfan.com

# SMS (Twilio)
TWILIO_ACCOUNT_SID=votre_account_sid
TWILIO_AUTH_TOKEN=votre_auth_token
TWILIO_PHONE_NUMBER=+33123456789

# URLs Frontend
FRONTEND_URL=http://localhost:3000
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session
SESSION_SECRET=votre_session_secret_ultra_securise

# Logging
LOG_LEVEL=info
LOG_FILE=logs/paiecashfan.log

# Stripe (Paiements)
STRIPE_SECRET_KEY=sk_test_votre_cle_stripe
STRIPE_PUBLISHABLE_KEY=pk_test_votre_cle_publique_stripe

# PayPal
PAYPAL_CLIENT_ID=votre_client_id_paypal
PAYPAL_SECRET=votre_secret_paypal
PAYPAL_MODE=sandbox

# Cloudinary (Upload images/NFT)
CLOUDINARY_CLOUD_NAME=votre_cloud_name
CLOUDINARY_API_KEY=votre_api_key
CLOUDINARY_API_SECRET=votre_api_secret
